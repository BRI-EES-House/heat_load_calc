    "            })\n",
    "        else:\n",
    "            if d_element['A'] is not None:\n",
    "                AMain=d_element['A']*rMain\n",
    "                AOther=d_element['A']*rOther\n",
    "                ANon=d_element['A']*rNon\n",
    "            else:\n",
